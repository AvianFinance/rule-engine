{
    "function_name" : "updateRentNFT",
    "input_parameters" : "address nftAddress, uint256 tokenId, uint256 price",
    "visibility" : "external",
    "state_mutability" : "none",
    "modifiers" : ["nonReentrant","isRentListed(nftAddress, tokenId)"],
    "returns" : "string memory",
    "requires" : [["IERC721(nftAddress).ownerOf(tokenId) == msg.sender","Not owner of nft"], ["price > 0", "Price should be greater than 0"]],
    "body" : [["updateRentalList",["nftAddress","tokenId"]]],
    "events" : ["RentNftListed(listing.owner, listing.user, nftAddress, tokenId, price, listing.expires)"],
    "return_line" : "'NFT Successfully listed'"
}

