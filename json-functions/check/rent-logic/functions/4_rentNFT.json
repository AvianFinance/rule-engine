{
    "function_name" : "rentNFT",
    "input_parameters" : "address nftAddress, uint256 tokenId, uint64 numDays",
    "visibility" : "external",
    "state_mutability" : "payable",
    "modifiers" : ["nonReentrant","isRentListed(nftAddress, tokenId)"],
    "returns" : "string memory",
    "requires" : [["MarketplaceIsApproved(nftAddress, tokenId)","Marketplace is not aproved"],["numDays <= _maxInstallments", "Maximum of 10 rental days are allowed"],["isNotRented(nftAddress, tokenId)", "NFT Already rented"]],
    "body" : [["load_listing",["rent","nftAddress","tokenId"]],["rent_nft",["nftAddress","tokenId"]],["pay_listing_fee",["listing.owner","rentalFee"]],["unlistNFT",["nftAddress","tokenId"]]],
    "events" : ["RentNftRented(IERC721(nftAddress).ownerOf(tokenId), msg.sender, nftAddress, tokenId, expires, rentalFee)"],
    "return_line" : "'NFT successfully rented'"
}


