{
    "function_name" : "listNFT",
    "input_parameters" : "address nftAddress, uint256 tokenId, uint256 price",
    "visibility" : "external",
    "state_mutability" : "payable",
    "modifiers" : ["nonReentrant", "notRentListed(nftAddress, tokenId)"],
    "returns" : "string memory",
    "requires" : [["MarketplaceIsApproved(nftAddress, tokenId)","Marketplace is not aproved"],["isRentableNFT(nftAddress)", "Contract is not an ERC4907"],["IERC721(nftAddress).ownerOf(tokenId) == msg.sender","Not owner of nft"], ["price > 0", "Price should be greater than 0"]],
    "body" : [["pay_listing_fee",["marketOwner","_listingFee"]],["write_listing_rental",["nftAddress","tokenId","price"]]],
    "events" : ["RentNftListed(IERC721(nftAddress).ownerOf(tokenId), address(0), nftAddress, tokenId, price, 0)"],
    "return_line" : "'NFT Successfully listed'"
}